text/html;\
    chromium-browser '%s' & sleep 1;\
    description=HTML Text;\
    test=test -n "$DISPLAY";\
    nametemplate=%s.html;
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -dump %s; nametemplate=%s.html; copiousoutput

video/*; vlc %s

image/*; xviewer %s

application/pdf; zathura %s; copiousoutput
